# This is Git's per-user configuration file.
[user]
  name = David Stalnaker
  email = david.stalnaker@gmail.com
[core]
  editor = /usr/bin/vim
  excludesfile = ~/.gitignore
[alias]
  c = commit
  cm = commit --message
  amend = commit -a --amend --no-edit
  reword = commit --amend

  s = status -sb
  d = diff
  du = diff @{upstream}
  a = add
  co = checkout
  b = branch
  l = log --oneline --graph
  f = fetch
  p = push
  pu = pull
  cp = cherry-pick
  su = submodule update
  j = checkout HEAD~1
[color]
  ui = auto
[rerere]
  enabled = 1
[pull]
  rebase = true
[init]
	defaultBranch = main
